<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="org.postgresql.Driver"
                        connectionURL="jdbc:postgresql://localhost:5432/10ch"
                        userId="postgres"
                        password="postgres"/>
        <javaModelGenerator targetPackage="com.example.ch.model" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="com.example.ch.mapper" targetProject="src/main/resources"/>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.ch.mapper" targetProject="src/main/java"/>
        <table schema="public" tableName="comments"/>
 
    </context>
</generatorConfiguration>